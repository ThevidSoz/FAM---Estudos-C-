#include <studio.h>
#include <conio.h>

main()
{
	int num, quad; // Definição de váriáveis
	
	printf("Digite um valor inteiro: "); // Mensagem na tela
	scanf("%d, &num"); // Recebe um número
	quad = num * num; // Faz o calculo
	
	printf("O quadrado de %d = %d\n", num, quad); // exibe o resultado concatenado
	printf("Pressione qualquer tecla para finalizar: "); // Mensagem na tela
	getch(); // Aguarda pressionar uma tecla
}
